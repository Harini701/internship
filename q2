import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner a=new Scanner(System.in);
        System.out.println("enter number=");
        int num=a.nextInt();
        for(int i=2;i<11;i=i+2) {
            System.out.println(num + "x" + i + "=" + num * i);
        }
    }
}
